<h1>All Contacts</h1>

<% @contacts.each do |contact| %>

  <h4>Full Name: <a href="/contacts/<%= contact.id %>"><%= contact.full_name %></a></h4>
  <h4>Email: <%= contact.email %></h4>
  <h4>Phone Number: <%= contact.phone_number %></h4>
  <p>-----------------------------------------------------------</p>

<% end %>